{
    "jupyter-lite-schema-version": 0,
    "jupyter-config-data": {
        "appName": "Online Laboratory for Climate Science and Meteorology",
        "appVersion": "0.1.0",
        "faviconUrl": "/static/favicons/favicon.ico",
        "exposeAppInBrowser": false,
        "disabledExtensions": ["@jupyterlite/server-extension:service-worker"],
        "enableMemoryStorage": true,
        "contentsStorageDrivers": ["memoryStorageDriver"],
        "litePluginSettings": {
            "@jupyterlite/pyodide-kernel-extension:kernel": {
                "pyodideUrl": "/static/pyodide/pyodide.js"
            }
        }
    }
}
